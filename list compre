if __name__ == '__main__':
    x = int(input())
    y = int(input())
    z = int(input())
    n = int(input())
    list1=[]
    for s in range(x+1):
        for a in range(y+1):
            for l in range(z+1):
                if s+a+l!=n:
                    list1.append([s,a,l])
    print(list1)
                 
